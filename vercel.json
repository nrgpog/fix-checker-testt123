{
  "rewrites": [
    {
      "source": "/api/basic/check",
      "destination": "/api/basic/check",
      "has": [
        {
          "type": "method",
          "value": "POST"
        }
      ]
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/gen",
      "destination": "/gen.html"
    },
    {
      "source": "/(.*)",
      "destination": "/chk.html"
    }
  ]
}
