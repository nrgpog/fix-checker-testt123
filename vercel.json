{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/gen",
      "destination": "/gen.html"
    },
    {
      "source": "/(.*)",
      "destination": "/chk.html"
    }
  ],
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ]
}
