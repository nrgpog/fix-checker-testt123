{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/gen",
      "dest": "/public/gen.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/chk.html"
    }
  ]
}
